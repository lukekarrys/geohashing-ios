{
  "name": "geohashing",
  "description": "A geohashing app for iOS built with React Native",
  "version": "1.0.0",
  "dependencies": {
    "geo-graticule": "^2.0.3",
    "geohash-coordinates": "^1.1.3",
    "lodash": "^4.11.1",
    "react": "^0.14.8",
    "react-native": "^0.23.1",
    "react-native-blur": "^0.7.11",
    "react-native-button": "^1.4.2",
    "react-native-drawer": "^1.16.7",
    "react-native-geocoder": "^0.3.1",
    "react-native-vector-icons": "^1.3.4",
    "react-pure-render": "^1.0.2",
    "zero-fill": "^2.2.3"
  },
  "devDependencies": {
    "@lukekarrys/eslint-config": "^2.1.2",
    "babel-eslint": "^6.1.2",
    "eslint": "^3.2.2",
    "eslint-plugin-classes": "^0.1.1",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-react": "^6.0.0",
    "git-validate": "^2.1.4"
  },
  "enginges": {
    "node": "^5.0.0"
  },
  "main": "README.md",
  "pre-commit": [
    "lint",
    "validate"
  ],
  "private": true,
  "scripts": {
    "bundle": "npm run bundle.ios",
    "bundle.ios": "NODE_ENV=production node ./node_modules/react-native/local-cli/cli.js bundle --entry-file index.ios.js --platform ios --dev false --bundle-output ios/geohashing/main.jsbundle && NODE_ENV=production node lib/release",
    "lint": "eslint .",
    "postinstall": "npm run rm.babelrc",
    "rm.babelrc": "find node_modules -type f -name .babelrc | grep -v /react-native/ | xargs rm",
    "start": "node lib/release && node ./node_modules/react-native/local-cli/cli.js start",
    "validate": "npm ls >/dev/null",
    "xcode": "open ./ios/geohashing.xcodeproj"
  }
}
