{
  "name": "geohashing",
  "description": "A geohashing app for iOS built with React Native",
  "version": "1.0.0",
  "dependencies": {
    "geo-graticule": "^2.0.3",
    "geohash-coordinates": "^1.1.3",
    "lodash": "^3.10.1",
    "react-native": "^0.16.0",
    "react-native-blur": "^0.7.6",
    "react-native-button": "^1.3.1",
    "react-native-drawer": "^1.10.1",
    "react-native-geocoder": "^0.1.4",
    "react-native-icons": "^0.7.0",
    "react-pure-render": "^1.0.2",
    "zero-fill": "^2.2.1"
  },
  "devDependencies": {
    "@lukekarrys/eslint-config": "^1.0.36",
    "babel-eslint": "^5.0.0-beta6",
    "eslint": "^1.10.3",
    "eslint-plugin-classes": "^0.1.1",
    "eslint-plugin-react": "^3.11.3",
    "precommit-hook": "^3.0.0",
    "react-native-cli": "^0.1.7"
  },
  "enginges": {
    "node": "^5.0.0"
  },
  "main": "README.md",
  "pre-commit": [
    "lint",
    "validate"
  ],
  "private": true,
  "scripts": {
    "bundle": "npm run bundle.ios && npm run bundle.android",
    "bundle.android": "echo 'Not building Android'",
    "bundle.ios": "NODE_ENV=production react-native bundle --entry-file index.ios.js --platform ios --dev false --bundle-output ios/geohashing/main.jsbundle && node lib/release",
    "lint": "eslint .",
    "postinstall": "npm run rm.babelrc && npm run rm.jshint",
    "rm.babelrc": "find node_modules -type f -name .babelrc | grep -v /react-native/ | xargs rm",
    "rm.jshint": "find . -type f -name '.jshint*' -maxdepth 1 | xargs rm",
    "start": "node lib/release && react-native start",
    "validate": "npm ls >/dev/null",
    "xcode": "open ./ios/geohashing.xcodeproj"
  }
}
