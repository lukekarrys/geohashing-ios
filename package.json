{
  "name": "geohashing",
  "description": "A geohashing app for iOS built with React Native",
  "version": "1.0.0",
  "dependencies": {
    "fbjs": "^0.8.14",
    "geo-graticule": "^2.0.5",
    "geohash-coordinates": "^1.1.6",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "react": "16.0.0-alpha.12",
    "react-native": "^0.48.0",
    "react-native-blur": "^3.1.3",
    "react-native-button": "^2.1.0",
    "react-native-dismiss-keyboard": "^1.0.0",
    "react-native-drawer": "^2.3.0",
    "react-native-geocoder": "^0.5.0",
    "react-native-maps": "^0.17.0",
    "react-native-vector-icons": "^4.3.0",
    "zero-fill": "^2.2.3"
  },
  "devDependencies": {
    "@lukekarrys/eslint-config": "^6.0.0",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^21.0.2",
    "eslint": "^4.4.1",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-classes": "^0.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-mysticatea": "^4.2.4",
    "eslint-plugin-prefer-object-spread": "^1.2.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.2.1",
    "git-validate": "^2.2.2",
    "jest": "^21.0.1",
    "react-test-renderer": "16.0.0-alpha.12",
    "rimraf": "^2.6.1"
  },
  "enginges": {
    "node": "^6.0.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "main": "README.md",
  "pre-commit": [
    "lint",
    "validate",
    "test"
  ],
  "private": true,
  "scripts": {
    "cli": "node node_modules/react-native/local-cli/cli.js",
    "ios": "npm run cli -- run-ios",
    "lint": "eslint .",
    "start": "npm run cli -- start",
    "test": "jest",
    "test:build": "NODE_ENV=production npm run cli -- bundle --entry-file index.ios.js --platform ios --dev false --bundle-output build.test.js && rimraf build.test.js*",
    "validate": "npm ls >/dev/null",
    "xcode": "open ./ios/geohashing.xcodeproj"
  }
}
